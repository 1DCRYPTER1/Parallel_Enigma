<LinearLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	xmlns:tools="http://schemas.android.com/tools"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	android:orientation="vertical">
	<LinearLayout
		android:id="@+id/linear5"
		android:layout_width="match_parent"
		android:layout_height="match_parent"
		android:padding="8dp"
		android:background="#001A29"
		android:gravity="center_horizontal|center_vertical"
		android:orientation="vertical"
		android:layout_gravity="center_horizontal|center_vertical">
		<TextView
			android:id="@+id/textview1"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:padding="8dp"
			android:text="Caution: Circuits are very fragile."
			android:textSize="14sp"
			android:textColor="#F50057"
			android:translationY="-70.0dp"/>
		<LinearLayout
			android:id="@+id/linear9"
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:gravity="center_horizontal"
			android:orientation="horizontal">
			<ImageView
				android:id="@+id/imageview1"
				android:layout_width="120dp"
				android:layout_height="120dp"
				android:src="@drawable/mazp_1"
				android:scaleType="fitXY"/>
			<ImageView
				android:id="@+id/imageview2"
				android:layout_width="120dp"
				android:layout_height="120dp"
				android:src="@drawable/mazp_2"
				android:scaleType="fitXY"
				android:rotation="90"/>
			<ImageView
				android:id="@+id/imageview3"
				android:layout_width="120dp"
				android:layout_height="120dp"
				android:src="@drawable/mazp_3"
				android:scaleType="fitXY"
				android:rotation="180"/>
		</LinearLayout>
		<LinearLayout
			android:id="@+id/linear6"
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:gravity="center_horizontal"
			android:orientation="horizontal">
			<ImageView
				android:id="@+id/imageview4"
				android:layout_width="120dp"
				android:layout_height="120dp"
				android:src="@drawable/mazp_4"
				android:scaleType="fitXY"
				android:rotation="90"/>
			<ImageView
				android:id="@+id/imageview5"
				android:layout_width="120dp"
				android:layout_height="120dp"
				android:src="@drawable/mazp_5"
				android:scaleType="fitXY"
				android:rotation="270"/>
			<ImageView
				android:id="@+id/imageview6"
				android:layout_width="120dp"
				android:layout_height="120dp"
				android:src="@drawable/mazp_6"
				android:scaleType="fitXY"
				android:rotation="270"/>
		</LinearLayout>
		<LinearLayout
			android:id="@+id/linear7"
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:gravity="center_horizontal"
			android:orientation="horizontal">
			<ImageView
				android:id="@+id/imageview7"
				android:layout_width="120dp"
				android:layout_height="120dp"
				android:src="@drawable/mazp_7"
				android:scaleType="fitXY"
				android:rotation="90"/>
			<ImageView
				android:id="@+id/imageview8"
				android:layout_width="120dp"
				android:layout_height="120dp"
				android:src="@drawable/mazp_8"
				android:scaleType="fitXY"/>
			<ImageView
				android:id="@+id/imageview9"
				android:layout_width="120dp"
				android:layout_height="120dp"
				android:src="@drawable/mazp_9"
				android:scaleType="fitXY"
				android:rotation="180"/>
		</LinearLayout>
		<Button
			android:id="@+id/rstbtn"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:padding="8dp"
			android:text="Reset"
			android:textSize="16sp"
			android:textColor="#000000"
			android:translationY="50.0dp"
			android:layout_gravity="center_horizontal"/>
	</LinearLayout>
</LinearLayout>
